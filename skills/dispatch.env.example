# Copy to <workspace>/skills/dispatch.env.local
# Legacy fallback still supported: ~/.config/openclaw/dispatch.env

# Project root for /dispatch and /dispatchi
REPOS_ROOT=/home/miniade/repos

# Result + launcher logs
RESULTS_BASE=/home/miniade/clawd/data/claude-code-results
LAUNCH_LOG_DIR=/home/miniade/clawd/data/dispatch-launch

# Claude/OpenClaw binaries and config
CLAUDE_CODE_BIN=/home/miniade/.local/bin/claude
OPENCLAW_BIN=/home/miniade/.npm-global/bin/openclaw
OPENCLAW_CONFIG=/home/miniade/.openclaw/openclaw.json
OPENCLAW_TELEGRAM_ACCOUNT=coder

# Source dispatch engine scripts
DISPATCH_REPO=/home/miniade/repos/claude-code-dispatch

# Behavior defaults
DISPATCH_PERMISSION_MODE=bypassPermissions
DISPATCH_TIMEOUT_SEC=7200
# Keep empty unless explicitly needed
DISPATCH_TEAMMATE_MODE=

# dispatchi / ralph-loop defaults
DISPATCHI_MAX_ITERATIONS=20
DISPATCHI_COMPLETION_PROMISE=COMPLETE
AUTO_EXIT_ON_COMPLETE=1
AUTO_EXIT_GRACE_SEC=20
AUTO_EXIT_MAX_WAIT_SEC=21600
AUTO_EXIT_POLL_SEC=5

# Group routing
CODEHOOK_GROUP_DEFAULT=-1002547895616
TELEGRAM_GROUP=-1002547895616
